{
  "extends": "airbnb-base",
  "rules": {
    "new-cap": 0,
    "func-names": 0,
    "space-before-function-paren": 0,
    "no-console": 0,
    "prefer-rest-params": 0,
    "import/no-extraneous-dependencies": 0,
    "no-confusing-arrow": 0,
    "no-underscore-dangle": 0,
    "strict": 0
  },
  "globals": {
    "describe": true,
    "it": true
  }
}